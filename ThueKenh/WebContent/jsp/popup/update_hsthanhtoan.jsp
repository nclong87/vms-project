<%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><%@ taglib prefix="s" uri="/struts-tags"%><s:url action="updateTrangThaiThanhToan" namespace="/ajax" id="updateTrangThaiThanhToanURL" /><%	String contextPath = request.getContextPath();%><form id="formUpdate" onsubmit="return false"><table width="500px" style="margin-top: 10px;">	<tbody id="display">	<tr>		<td></td>		<td style="text-align:left" id="msg">		</td>	</tr>	<tr>		<td width="150px" style="text-align:right">			Số hồ sơ :		</td>		<td style="text-align:left">			 <input type="text" name="sohoso" id="sohoso" readonly="true" value="<s:property value='json.sohoso'/>"/>		</td>	</tr>	<tr>		<td style="text-align:right">			Ngày ký UNC :		</td>		<td style="text-align:left">			 <input type="text" class="date" name="ngaykyunc" id="ngaykyunc"/>		</td>	</tr>	<tr>		<td style="text-align:right">			Ngày chuyển khoản :		</td>		<td style="text-align:left">			 <input type="text" class="date" name="ngaychuyenkhoan" id="ngaychuyenkhoan"/>		</td>	</tr>	<tr>		<td style="text-align:center" colspan="2">			<input id="btSubmit" class="button" type="button" value="Chuyển trạng thái"/>		</td>	</tr>	</tbody></table></form><script>var popup = {	message : function(msg,type) {		if(msg == '') {			$("#formUpdate #msg").html('');			return;		}		if(type == 1) {			$("#formUpdate #msg").html('<div class="ui-state-highlight ui-corner-all" style=" padding: 0pt 0.7em; text-align: left;"><p style="padding: 5px;"><span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span><strong>Success! </strong> '+msg+'</p></div>');		} else {			$("#formUpdate #msg").html('<div style="padding: 0pt 0.7em; text-align: left;" class="ui-state-error ui-corner-all"><p style="padding: 5px;"><span style="float: left; margin-right: .3em;" class="ui-icon ui-icon-alert"></span><strong>Error : </strong> '+msg+'</p></div>');		}	}}$(document).ready(function(){		$( "input.date" ).datepicker({		showButtonPanel: true,		dateFormat : "dd/mm/yy"	});	$("#btSubmit").click(function(){		var button = this;		var dataString = $("#formUpdate").serialize();		button.disabled = true;		$.ajax({			url: "${updateTrangThaiThanhToanURL}",			type:'POST',			cache : false,			data:dataString,			success:function(response){				if(response.status == 0) {					button.disabled = false;					popup.message("Có lỗi xảy ra, vui lòng thử lại sau!",0);					return;				} else if (response.status == 1) {					popup.message("Chuyển trạng thái thành công!",1);					oTable.fnDraw(false);					return;				}				popup.message("Có lỗi xảy ra, vui lòng thử lại sau!",0);			},			error:function(response){				button.disabled = false;				alert("Server is too busy, please try again!");			}		});		return false;	});});</script>