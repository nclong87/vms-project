<%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><%@ taglib prefix="s" uri="/struts-tags"%><s:url action="doUploadTuyenkenh2" namespace="/import" id="doUploadTuyenkenh2URL" /><%	String contextPath = request.getContextPath();%><form onsubmit="return false" enctype="multipart/form-data" method="post" id="frmUpload" action="undefined" encoding="multipart/form-data"><table width="500px" style="margin-top: 30px;">	<tbody id="display">	<tr>		<td width="175" style="text-align:right">			Chọn file Excel ( <a href="<%=contextPath%>/files/TuyenKenhExcelMau2.xls">file mẫu</a> ) :		</td>		<td style="text-align:left">			<input type="file" name="fileupload" id="fileupload"/>			<input id="btUpload" class="button" style="display:none" type="button" value="Upload"/>		</td>	</tr>	</tbody></table></form><script>$(document).ready(function(){		$("#fileupload").change(function(){		$("#btUpload").show();	});	$("#btUpload").click(function(){		this.disabled = true;		$('#frmUpload').submit();	});	$('#frmUpload').ajaxForm({ 		url:  "${doUploadTuyenkenh2URL}",		type: "post",		dataType : "json",		success:    function(response) { 			if(response.result == "ERROR") {				alert(response.data);				return;			}			if(response.result == "OK") {				$.each(response.data,function(){					if(this.id != "") {						if($("#frmTinhGiaTriPhuLuc #tuyenkenh_"+this.id).length == 0) {							addRow(this);						}					}				});				$("#dialog").dialog("close");				rowChanges(1);				return;			}			alert("Kết nối bị lỗi, vui lòng thử lại!");		},		error : function(data) {			alert("Kết nối bị lỗi, vui lòng thử lại!");		}	});});</script>