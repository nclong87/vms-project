<s:url action="updateAccount" namespace="/user" id="updateAccountURL"/><div id="dialog" title="Welcome to VMS"><center>Loading...</center></div><div id="top">	<div id="head">		<h1 class="logo">			<a href="<%= request.getContextPath() %>"></a>		</h1>		<div class="head_memberinfo">			<span class='memberinfo_span'>				 Welcome <strong><s:property value="#session.SESS_USERLOGIN.username"/></strong>			</span>						<span class='memberinfo_span'>				<span title="Cập nhật thông tin tài khoản của bạn" style="cursor:pointer" onclick="ShowWindow('Cập nhật tài khoản',720,400,'${updateAccountURL}',false);">Your account</span>			</span>			<span>				<a href="${doLogoutURL}">Logout</a>			</span>		</div><!--end head_memberinfo-->	</div><!--end head-->	<s:property value="#session.SESS_MENU" escape="false"/></div>